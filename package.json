{
  "name": "mycokv-node",
  "version": "0.0.1",
  "description": "A Node.js driver for MycoKV",
  "main": "./lib/index.js",
  "scripts": {
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outdir lib/cjs",
    "build": "yarn build:esm && yarn build:cjs",
    "prepublishOnly": "yarn build",
    "setup:e2e": "yarn build && yarn link && cd e2e && yarn install && yarn link mycokv-node && cd ..",
    "test:e2e": "(cd e2e && yarn start)"
  },
  "author": "WVAviator",
  "license": "MIT",
  "files": [
    "/lib"
  ]
}
